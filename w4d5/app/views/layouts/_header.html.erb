<header>
  <img class="logo" src="https://cdn3.iconfinder.com/data/icons/ultimate-social/150/41_itunes-512.png"/>
  <section class="title"> dope music app </section>
  <section class="user-options">
    <% if current_user %>
    <span class="user-email"><%= current_user.email %></span>
      <%= button_to "Log Out", session_url, method: :delete  %>
    <% else %>
      <%= button_to "Sign Up", new_user_url, method: :get  %>
      <%= button_to "Sign In", new_session_url, method: :get  %>
    <% end %>
  </section>
</header>
